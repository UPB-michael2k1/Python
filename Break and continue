# Example of break and continue statement 
# using for loop 
print( "The break example")

for i in range(1,6):
    if i == 3:
        break
    print("inside the loop", i)
    
print("Outside the loop")

print("The continue example")

for i in range ( 1,6):
    if i == 3 :
        continue
    print("inside the loop", i)
print("outside the loop") 
